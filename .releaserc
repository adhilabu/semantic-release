{
    "release": {
        "branches": [
            {
                "name": "main"
            },
            {
                "name": "next",
                "channel": "next"
            },
            {
                "name": "alpha",
                "channel": "alpha",
                "prerelease": true
            },
            {
                "name": "beta",
                "channel": "beta",
                "prerelease": true
            },
            {
                "name": "1.x",
                "range": "1.x",
                "channel": "1.x"
            },
            {
                "name": "2.x",
                "range": "2.x",
                "channel": "2.x"
            },
        ]
    },
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/github",
            {
                "assets": [
                    "dist/**"
                ]
            }
        ],
        "@semantic-release/git"
    ],
    "preset": "angular",
    "presetConfig": {
        "types": [
            {
                "type": "feat",
                "section": "Features"
            },
            {
                "type": "feature",
                "section": "Features"
            },
            {
                "type": "fix",
                "section": "Bug Fixes"
            },
            {
                "type": "perf",
                "section": "Performance Improvements"
            },
            {
                "type": "revert",
                "section": "Reverts"
            },
            {
                "type": "docs",
                "section": "Documentation"
            },
            {
                "type": "style",
                "section": "Styles"
            },
            {
                "type": "chore",
                "section": "Miscellaneous Chores"
            },
            {
                "type": "refactor",
                "section": "Code Refactoring"
            },
            {
                "type": "test",
                "section": "Tests"
            },
            {
                "type": "build",
                "section": "Build System"
            },
            {
                "type": "ci",
                "section": "Continuous Integration"
            }
        ]
    }
}